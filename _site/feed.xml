    &lt;tr&gt;
      &lt;td&gt;&lt;span style=&quot;font-size:1.5em;&quot;&gt;Binding:&lt;/span&gt;  &lt;/td&gt;
      &lt;td&gt;&lt;span style=&quot;font-size:1.5em;&quot;&gt;See notes&lt;/span&gt;&lt;/td&gt;
    &lt;/tr&gt;
    &lt;tr&gt;
      &lt;td&gt;&lt;span style=&quot;font-size:1.5em;&quot;&gt;Annotations:&lt;/span&gt;  &lt;/td&gt;
      &lt;td&gt;&lt;span style=&quot;font-size:1.5em;&quot;&gt;Underlining &amp;amp; handwritten marginalia&lt;/span&gt;&lt;/td&gt;
